0=CREATE TABLE administrator(id VARCHAR(10) PRIMARY KEY,name VARCHAR(10),sex CHAR(1));
1=CREATE TABLE student(id VARCHAR(10) PRIMARY KEY,name VARCHAR(10),sex CHAR(1));
2=CREATE TABLE teacher(id VARCHAR(10) PRIMARY KEY,name VARCHAR(10),sex CHAR(1));
3=CREATE TABLE course(id VARCHAR(10) PRIMARY KEY,name VARCHAR(10),credit INT,period INT);
4=CREATE TABLE class(id VARCHAR(10) PRIMARY KEY);
5=CREATE TABLE grade(id VARCHAR(10) PRIMARY KEY);
6=CREATE TABLE dept(id VARCHAR(10) PRIMARY KEY);
7=CREATE TABLE major(id VARCHAR(10) PRIMARY KEY);
8=CREATE TABLE teachclass(id VARCHAR(10) PRIMARY KEY);

9=CREATE TABLE student_class(student_id VARCHAR(10),class_id VARCHAR(10),PRIMARY KEY(student_id,class_id),FOREIGN KEY(student_id) REFERENCES student(id),FOREIGN KEY(class_id) REFERENCES class(id));
10=CREATE TABLE class_grade(class_id VARCHAR(10),grade_id VARCHAR(10),PRIMARY KEY(class_id,grade_id),FOREIGN KEY(class_id) REFERENCES class(id),FOREIGN KEY(grade_id) REFERENCES grade(id));
11=CREATE TABLE class_dept(class_id VARCHAR(10),dept_id VARCHAR(10),PRIMARY KEY(class_id,dept_id),FOREIGN KEY(class_id) REFERENCES class(id),FOREIGN KEY(dept_id) REFERENCES dept(id));
12=CREATE TABLE dept_major(dept_id VARCHAR(10),major_id VARCHAR(10),PRIMARY KEY(dept_id,major_id),FOREIGN KEY(dept_id) REFERENCES dept(id),FOREIGN KEY(major_id) REFERENCES major(id));
13=CREATE TABLE student_course(student_id VARCHAR(10),course_id VARCHAR(10),score DOUBLE,PRIMARY KEY(student_id,course_id),FOREIGN KEY(student_id) REFERENCES student(id),FOREIGN KEY(course_id) REFERENCES course(id));
14=CREATE TABLE teacher_course(id VARCHAR(10) PRIMARY KEY,teacher_id VARCHAR(10),course_id VARCHAR(10),FOREIGN KEY(teacher_id) REFERENCES teacher(id),FOREIGN KEY(course_id) REFERENCES course(id),FOREIGN KEY(id) REFERENCES teachclass(id));
15=CREATE TABLE student_teachclass(student_id VARCHAR(10),teachclass_id VARCHAR(10),PRIMARY KEY(student_id,teachclass_id),FOREIGN KEY(student_id) REFERENCES student(id),FOREIGN KEY(teachclass_id) REFERENCES teachclass(id));
16=CREATE TABLE teacher_teachclass(teacher_id VARCHAR(10),teachclass_id VARCHAR(10),PRIMARY KEY(teacher_id,teachclass_id),FOREIGN KEY(teacher_id) REFERENCES teacher(id),FOREIGN KEY(teachclass_id) REFERENCES teachclass(id));
17=CREATE TABLE teachclass_time(teachclass_id VARCHAR(10) PRIMARY KEY,time DATE,FOREIGN KEY(teachclass_id) REFERENCES teachclass(id));
